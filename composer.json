{
  "name": "ronijan/backup-db",
  "description": "The ability to full-backup your Data easily.",
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Ronijan",
      "email": "huseyinkhalil@web.de"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-mysqli": "*"
  },
  "require-dev": {},
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Ronijan\\BackupDatabase\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": []
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
