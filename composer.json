{
    "name": "ronijan/backup-db",
    "description": "The ability to backup your Data easily.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Ronijan",
            "email": "huseyinkhalil@web.de"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
      "ext-mysqli": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }, 
    "autoload": {
        "psr-4": {
            "Ronijan\\BackupDatabase\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/ExampleTest.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
